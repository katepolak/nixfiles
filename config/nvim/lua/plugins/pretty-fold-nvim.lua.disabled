return {
	'anuvyklack/pretty-fold.nvim',
	config = function()
	require('pretty-fold').setup {
			keep_indentation = false,
			fill_char = '━',
			sections = {
				left = {
					function() return string.rep('━', math.max(0, math.max(0, (vim.v.foldlevel - 1)) * vim.bo.shiftwidth - 3)) end, '━┫', 'content', '┣━━━━━━━┫ ', 'number_of_folded_lines', ' ┣'
				},
				right = {
					'┫ ', 'percentage', ' ┣━━',
				}
			}
		}
	end
}
